{
  "name": "mber",
  "version": "0.12.8",
  "type": "module",
  "description": "Modern, fast, non-backward compatible ember-cli replacement with Module Unification RFC",
  "author": "Izel Nakri",
  "license": "MIT",
  "main": "index.js",
  "engines": {
    "node": ">=13.8.0"
  },
  "bin": {
    "mber": "cli.js"
  },
  "scripts": {
    "build:loader": "cp ./node_modules/loader.js/dist/loader/loader.js ./vendor",
    "build:ember-source": "cp ./node_modules/ember-source/dist/ember-template-compiler.js ./vendor && cp ./node_modules/ember-source/dist/ember-testing.js ./vendor",
    "build:full-ember": "node scripts/build-ember-base.js",
    "build:no-ember-data": "node scripts/build-ember-base.js --exclude-ember-data",
    "build:test-support": "node scripts/build-test-support.js",
    "build:memserver": "node scripts/build-memserver.js",
    "build:fastboot-modules": "node scripts/transpile-fastboot-modules.js",
    "build": "npm run build:loader && npm run build:ember-source && npm run build:full-ember && npm run build:no-ember-data && npm run build:test-support && npm run build:memserver && npm run build:fastboot-modules",
    "prepack": "npm run build",
    "bin": "node cli.js",
    "test": "node_modules/.bin/ava --verbose"
  },
  "devDependencies": {
    "@ember-data/adapter": "3.20.0",
    "@ember-data/debug": "^3.20.0",
    "@ember-data/model": "3.20.0",
    "@ember-data/private-build-infra": "^3.20.0",
    "@ember-data/record-data": "3.20.0",
    "@ember-data/serializer": "3.20.0",
    "@ember-data/store": "3.20.0",
    "@ember/test-helpers": "1.7.1",
    "@glimmer/di": "^0.2.1",
    "@glimmer/resolver": "^0.4.3",
    "@typescript-eslint/parser": "3.6.1",
    "ava": "^3.10.1",
    "cors": "^2.8.5",
    "ember-cli-fastboot": "2.2.3",
    "ember-cli-test-loader": "^3.0.0",
    "ember-data": "3.20.0",
    "ember-devtools": "^6.0.0",
    "ember-load-initializers": "^2.1.1",
    "ember-qunit": "^4.6.0",
    "ember-resolver": "^6.0.0",
    "ember-source": "3.20.1",
    "ember-test-waiters": "2.1.2",
    "intercept-stdout": "^0.1.2",
    "loader.js": "^4.7.0",
    "mber-head": "^0.1.2",
    "morgan": "^1.10.0",
    "qunit": "2.10.1",
    "qunit-dom": "^1.2.0",
    "strip-ansi": "^6.0.0",
    "typescript": "^3.9.7",
    "whatwg-fetch": "^3.2.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/izelnakri/mber.git"
  },
  "dependencies": {
    "@babel/core": "^7.10.5",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-decorators": "^7.10.5",
    "@babel/plugin-transform-modules-amd": "^7.10.5",
    "@babel/plugin-transform-typescript": "^7.10.5",
    "@babel/preset-env": "^7.10.4",
    "@ember-data/canary-features": "^3.20.0",
    "@ember/ordered-set": "^2.0.3",
    "@ember/render-modifiers": "^1.0.2",
    "@glimmer/component": "^1.0.0",
    "abortcontroller-polyfill": "^1.4.0",
    "amd-name-resolver": "^1.3.1",
    "ansi-colors": "^4.1.1",
    "babel-eslint": "^10.1.0",
    "babel-plugin-debug-macros": "^0.3.2",
    "babel-plugin-ember-modules-api-polyfill": "^2.13.4",
    "babel-plugin-filter-imports": "^4.0.0",
    "babel-plugin-htmlbars-inline-precompile": "^4.1.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel6-plugin-strip-heimdall": "^6.0.1",
    "babelify": "^10.0.0",
    "browserify": "^16.5.1",
    "cheerio": "^1.0.0-rc.3",
    "chokidar": "^3.4.1",
    "compression": "^1.7.3",
    "eslint": "^7.4.0",
    "eslint-plugin-ember": "^8.9.1",
    "express": "^4.17.1",
    "fastboot": "^3.1.0",
    "fs-extra": "^9.0.1",
    "jquery": "^3.5.1",
    "jsdom": "^16.3.0",
    "klaw": "^3.0.0",
    "memserver": "^2.0.12",
    "mustache": "^4.0.1",
    "node-fetch": "^2.6.0",
    "ora": "^4.0.4",
    "puppeteer": "5.1.0",
    "sass": "1.26.10",
    "say": "^0.16.0",
    "terser": "4.8.0",
    "through2": "^4.0.2",
    "ws": "^7.3.1"
  },
  "ava": {
    "nodeArguments": [
      "--experimental-modules"
    ],
    "files": [
      "test/**/*",
      "!test/helpers/**/*"
    ],
    "require": [
      "./vendor/disable-colors"
    ],
    "timeout": "10m",
    "concurrency": 1,
    "failFast": true
  },
  "volta": {
    "node": "13.14.0"
  },
  "browserslist": [
    "last 1 chrome version"
  ]
}
