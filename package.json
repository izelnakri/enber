{
  "name": "mber",
  "version": "0.5.10",
  "description": "Modern, fast, non-backward compatible ember-cli replacement with Module Unification RFC",
  "main": "index.js",
  "engines": {
    "node": ">=10.0.0"
  },
  "bin": {
    "mber": "cli.js"
  },
  "scripts": {
    "build:loader": "cp ./node_modules/loader.js/dist/loader/loader.js ./vendor",
    "build:ember-source": "cp ./node_modules/ember-source/dist/ember-template-compiler.js ./vendor && cp ./node_modules/ember-source/dist/ember-testing.js ./vendor",
    "build:full-ember": "node -r esm scripts/build-ember-base",
    "build:no-ember-data": "node -r esm scripts/build-ember-base --exclude-ember-data",
    "build:test-support": "node -r esm scripts/build-test-support",
    "build:memserver": "node -r esm scripts/build-memserver",
    "build:fastboot-modules": "node -r esm scripts/transpile-fastboot-modules",
    "build": "npm run build:loader && npm run build:ember-source && npm run build:full-ember && npm run build:no-ember-data && npm run build:test-support && npm run build:memserver && npm run build:fastboot-modules",
    "prepack": "npm run build",
    "bin": "node -r esm cli.js",
    "test": "ava --verbose"
  },
  "devDependencies": {
    "@ember/test-helpers": "0.7.25",
    "@glimmer/resolver": "^0.4.3",
    "ava": "^1.0.0-beta.6",
    "cors": "^2.8.4",
    "ember-cli-fastboot": "^1.1.4-beta.1",
    "ember-cli-test-loader": "^2.2.0",
    "ember-data": "3.3.1",
    "ember-devtools": "^6.0.0",
    "ember-load-initializers": "^1.1.0",
    "ember-qunit": "^3.4.1",
    "ember-resolver": "^5.0.1",
    "ember-source": "3.3.1",
    "eslint": "^5.3.0",
    "intercept-stdout": "^0.1.2",
    "loader.js": "^4.2.3",
    "mber-head": "^0.1.0",
    "morgan": "^1.9.0",
    "qunit": "2.6.1",
    "strip-ansi": "^4.0.0",
    "whatwg-fetch": "^2.0.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/izelnakri/mber.git"
  },
  "author": "Izel Nakri",
  "license": "MIT",
  "dependencies": {
    "amd-name-resolver": "^1.2.0",
    "babel-core": "^6.26.3",
    "babel-plugin-debug-macros": "^0.1.11",
    "babel-plugin-ember-modules-api-polyfill": "^2.3.2",
    "babel-plugin-htmlbars-inline-precompile": "^1.0.0",
    "babel-plugin-transform-es2015-constants": "^6.1.4",
    "babel-plugin-transform-es2015-modules-amd": "^6.24.1",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "babel6-plugin-strip-heimdall": "^6.0.1",
    "babelify": "^8.0.0",
    "browserify": "^16.2.2",
    "chalk": "^2.4.1",
    "cheerio": "^1.0.0-rc.2",
    "chokidar": "^2.0.4",
    "compression": "^1.7.3",
    "esm": "^3.0.74",
    "express": "^4.16.3",
    "fastboot": "^1.2.0",
    "fs-extra": "^7.0.0",
    "jquery": "^3.3.1",
    "jsdom": "^11.12.0",
    "klaw": "^3.0.0",
    "memserver": "^1.1.7",
    "mustache": "^2.3.0",
    "node-sass": "4.9.3",
    "ora": "^3.0.0",
    "puppeteer": "^1.6.2",
    "say": "^0.15.0",
    "through2": "^2.0.3",
    "uglify-es": "^3.3.9",
    "ws": "^6.0.0"
  },
  "ava": {
    "require": [
      "esm"
    ],
    "concurrency": 1,
    "failFast": true
  }
}
