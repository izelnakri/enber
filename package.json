{
  "name": "mber",
  "version": "0.0.22",
  "description": "My hobby take on replacing ember-cli with parcel sometime in far future",
  "main": "lib/index.js",
  "engines": {
    "node": ">=10.0.0"
  },
  "bin": {
    "mber": "cli.js"
  },
  "scripts": {
    "build:jquery": "node -r esm scripts/global-jquery",
    "build:loader": "cp ./node_modules/loader.js/dist/loader/loader.js ./vendor",
    "build:ember-source": "cp ./node_modules/ember-source/dist/ember-template-compiler.js ./vendor && cp ./node_modules/ember-source/dist/ember-testing.js ./vendor",
    "build:welcome-page": "cp ./scripts/welcome-page.js ./vendor/welcome-page.js",
    "build:full-ember": "node -r esm scripts/build-ember-base",
    "build:no-ember-data": "node -r esm scripts/build-ember-base --exclude-ember-data",
    "build:no-jquery-ember": "node -r esm scripts/build-ember-base --exclude-jquery",
    "build:only-ember": "node -r esm scripts/build-ember-base --exclude-ember-data --exclude-jquery",
    "build": "npm run build:jquery && npm run build:loader && npm run build:ember-source && npm run build:welcome-page && npm run build:full-ember && npm run build:no-ember-data && npm run build:no-jquery-ember && npm run build:only-ember",
    "prepublish": "npm run build",
    "bin": "node -r esm cli.js",
    "test": "ava --verbose"
  },
  "devDependencies": {
    "ava": "^1.0.0-beta.4",
    "ember-data": "3.1.1",
    "ember-load-initializers": "^1.1.0",
    "ember-resolver": "^4.5.5",
    "ember-source": "^3.1.1",
    "eslint": "^4.19.1",
    "jquery": "^3.3.1",
    "loader.js": "^4.7.0",
    "mock-require": "^3.0.2",
    "rollup": "^0.58.2",
    "rollup-plugin-commonjs": "^9.1.0",
    "rollup-plugin-node-resolve": "^3.3.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/izelnakri/mber.git"
  },
  "author": "Izel Nakri",
  "license": "ISC",
  "dependencies": {
    "amd-name-resolver": "^1.2.0",
    "babel-core": "^6.26.3",
    "babel-plugin-ember-modules-api-polyfill": "^2.3.0",
    "babel-plugin-htmlbars-inline-precompile": "^0.2.4",
    "babel-plugin-transform-es2015-constants": "^6.1.4",
    "babel-plugin-transform-es2015-modules-amd": "^6.24.1",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-register": "^6.26.0",
    "babel6-plugin-strip-heimdall": "^6.0.1",
    "chalk": "^2.4.0",
    "esm": "^3.0.34",
    "fs-extra": "^5.0.0",
    "jsdom": "^11.8.0",
    "klaw": "^2.1.1",
    "mustache": "^2.3.0",
    "ora": "^2.1.0",
    "parcel-bundler": "^1.8.1",
    "rimraf": "^2.6.2",
    "simple-dom": "^1.4.0-alpha.82914663",
    "through2": "^2.0.3",
    "uglify-js": "^3.3.26"
  },
  "ava": {
    "require": [
      "esm"
    ]
  }
}
