{
  "name": "mber",
  "version": "0.8.2",
  "description": "Modern, fast, non-backward compatible ember-cli replacement with Module Unification RFC",
  "main": "index.js",
  "engines": {
    "node": ">=10.0.0"
  },
  "bin": {
    "mber": "cli.js"
  },
  "scripts": {
    "build:loader": "cp ./node_modules/loader.js/dist/loader/loader.js ./vendor",
    "build:ember-source": "cp ./node_modules/ember-source/dist/ember-template-compiler.js ./vendor && cp ./node_modules/ember-source/dist/ember-testing.js ./vendor",
    "build:full-ember": "node -r esm scripts/build-ember-base",
    "build:no-ember-data": "node -r esm scripts/build-ember-base --exclude-ember-data",
    "build:test-support": "node -r esm scripts/build-test-support",
    "build:memserver": "node -r esm scripts/build-memserver",
    "build:fastboot-modules": "node -r esm scripts/transpile-fastboot-modules",
    "build": "npm run build:loader && npm run build:ember-source && npm run build:full-ember && npm run build:no-ember-data && npm run build:test-support && npm run build:memserver && npm run build:fastboot-modules",
    "prepack": "npm run build",
    "bin": "node -r esm cli.js",
    "test": "node_modules/.bin/ava --verbose"
  },
  "devDependencies": {
    "@ember/test-helpers": "1.5.0",
    "@glimmer/resolver": "^0.4.3",
    "ava": "^1.4.1",
    "cors": "^2.8.5",
    "ember-cli-fastboot": "^2.0.4",
    "ember-cli-test-loader": "^2.2.0",
    "ember-data": "3.10.0",
    "ember-devtools": "^6.0.0",
    "ember-load-initializers": "^2.0.0",
    "ember-qunit": "^4.4.1",
    "ember-resolver": "^5.1.3",
    "ember-source": "3.10.0",
    "eslint": "^5.16.0",
    "eslint-plugin-ember": "^6.4.1",
    "intercept-stdout": "^0.1.2",
    "jquery": "^3.4.1",
    "loader.js": "^4.7.0",
    "mber-head": "^0.1.0",
    "morgan": "^1.9.1",
    "node-fetch": "2.6.0",
    "qunit": "2.9.2",
    "strip-ansi": "^5.2.0",
    "whatwg-fetch": "^3.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/izelnakri/mber.git"
  },
  "author": "Izel Nakri",
  "license": "MIT",
  "dependencies": {
    "@babel/core": "^7.4.4",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-proposal-decorators": "^7.4.4",
    "@babel/plugin-transform-modules-amd": "^7.2.0",
    "@babel/plugin-transform-typescript": "^7.4.4",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@babel/register": "^7.4.4",
    "@ember/ordered-set": "^2.0.3",
    "amd-name-resolver": "^1.3.1",
    "ansi-colors": "^3.2.4",
    "babel-plugin-debug-macros": "^0.3.1",
    "babel-plugin-ember-modules-api-polyfill": "^2.9.0",
    "babel-plugin-filter-imports": "^3.0.0",
    "babel-plugin-htmlbars-inline-precompile": "^1.0.0",
    "babel-plugin-module-resolver": "^3.2.0",
    "babel6-plugin-strip-heimdall": "^6.0.1",
    "babelify": "^10.0.0",
    "browserify": "^16.2.3",
    "cheerio": "^1.0.0-rc.3",
    "chokidar": "^3.0.0",
    "compression": "^1.7.3",
    "esm": "^3.2.25",
    "express": "^4.17.0",
    "fastboot": "^2.0.1",
    "fs-extra": "^8.0.1",
    "jsdom": "^13.2.0",
    "klaw": "^3.0.0",
    "memserver": "^1.2.7",
    "mustache": "^3.0.1",
    "sass": "1.20.1",
    "ora": "^3.4.0",
    "puppeteer": "1.16.0",
    "say": "^0.15.0",
    "through2": "^3.0.0",
    "uglify-es": "^3.3.9",
    "ws": "^7.0.0"
  },
  "ava": {
    "require": [
      "esm",
      "./disable-colors"
    ],
    "concurrency": 1,
    "failFast": true
  }
}
