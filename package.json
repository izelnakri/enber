{
  "name": "mber",
  "version": "0.2.12",
  "description": "Modern, fast, non-backward compatible ember-cli replacement with Module Unification RFC",
  "main": "index.js",
  "engines": {
    "node": ">=10.0.0"
  },
  "bin": {
    "mber": "cli.js"
  },
  "scripts": {
    "build:jquery": "node -r esm scripts/global-jquery",
    "build:loader": "cp ./node_modules/loader.js/dist/loader/loader.js ./vendor",
    "build:ember-source": "cp ./node_modules/ember-source/dist/ember-template-compiler.js ./vendor && cp ./node_modules/ember-source/dist/ember-testing.js ./vendor",
    "build:full-ember": "node -r esm scripts/build-ember-base",
    "build:no-ember-data": "node -r esm scripts/build-ember-base --exclude-ember-data",
    "build:no-jquery-ember": "node -r esm scripts/build-ember-base --exclude-jquery",
    "build:only-ember": "node -r esm scripts/build-ember-base --exclude-ember-data --exclude-jquery",
    "build:test-support": "node -r esm scripts/build-test-support",
    "build:fastboot-modules": "node -r esm scripts/transpile-fastboot-modules",
    "build:memserver": "node -r esm scripts/build-memserver",
    "build": "npm run build:memserver && npm run build:jquery && npm run build:loader && npm run build:ember-source && npm run build:full-ember && npm run build:no-ember-data && npm run build:no-jquery-ember && npm run build:only-ember && npm run build:test-support && npm run build:fastboot-modules",
    "prepack": "npm run build",
    "bin": "node -r esm cli.js",
    "test": "ava --verbose"
  },
  "devDependencies": {
    "@ember/test-helpers": "0.7.25",
    "@glimmer/resolver": "^0.4.3",
    "ava": "^1.0.0-beta.6",
    "ember-cli-test-loader": "^2.2.0",
    "ember-load-initializers": "^1.1.0",
    "ember-qunit": "^3.4.1",
    "ember-resolver": "^4.5.6",
    "ember-source": "3.2.2",
    "eslint": "^5.0.0",
    "jquery": "^3.3.1",
    "loader.js": "^4.2.3",
    "mber-head": "^0.1.0",
    "mock-require": "^3.0.2",
    "qunit": "2.6.1",
    "rollup": "^0.61.1",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-virtual": "^1.0.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/izelnakri/mber.git"
  },
  "author": "Izel Nakri",
  "license": "MIT",
  "dependencies": {
    "amd-name-resolver": "^1.2.0",
    "babel-core": "^6.26.3",
    "babel-plugin-ember-modules-api-polyfill": "^2.3.2",
    "babel-plugin-htmlbars-inline-precompile": "^0.2.5",
    "babel-plugin-transform-es2015-constants": "^6.1.4",
    "babel-plugin-transform-es2015-modules-amd": "^6.24.1",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "babel6-plugin-strip-heimdall": "^6.0.1",
    "babelify": "^8.0.0",
    "browserify": "^16.2.2",
    "chalk": "^2.4.1",
    "cheerio": "^1.0.0-rc.2",
    "chokidar": "^2.0.4",
    "compression": "^1.7.2",
    "ember-cli-fastboot": "^1.1.4-beta.1",
    "ember-data": "3.1.1",
    "ember-devtools": "^6.0.0",
    "esm": "^3.0.52",
    "express": "^4.16.3",
    "fastboot": "^1.2.0",
    "fs-extra": "^6.0.1",
    "jsdom": "^11.11.0",
    "klaw": "^2.1.1",
    "memserver": "^1.1.6",
    "mustache": "^2.3.0",
    "node-sass": "4.9.0",
    "ora": "^2.1.0",
    "puppeteer": "^1.5.0",
    "rimraf": "^2.6.2",
    "through2": "^2.0.3",
    "uglify-js": "^3.4.1",
    "whatwg-fetch": "^2.0.4",
    "ws": "^5.2.0"
  },
  "ava": {
    "require": [
      "esm"
    ]
  }
}
