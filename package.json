{
  "name": "mber",
  "version": "0.8.18",
  "type": "module",
  "description": "Modern, fast, non-backward compatible ember-cli replacement with Module Unification RFC",
  "author": "Izel Nakri",
  "license": "MIT",
  "main": "index.js",
  "engines": {
    "node": ">=12.4.0"
  },
  "bin": {
    "mber": "cli.js"
  },
  "scripts": {
    "build:loader": "cp ./node_modules/loader.js/dist/loader/loader.js ./vendor",
    "build:ember-source": "cp ./node_modules/ember-source/dist/ember-template-compiler.js ./vendor && cp ./node_modules/ember-source/dist/ember-testing.js ./vendor",
    "build:full-ember": "node --experimental-modules scripts/build-ember-base.js",
    "build:no-ember-data": "node --experimental-modules scripts/build-ember-base.js --exclude-ember-data",
    "build:test-support": "node --experimental-modules scripts/build-test-support.js",
    "build:memserver": "node --experimental-modules scripts/build-memserver.js",
    "build:fastboot-modules": "node --experimental-modules scripts/transpile-fastboot-modules.js",
    "build": "npm run build:loader && npm run build:ember-source && npm run build:full-ember && npm run build:no-ember-data && npm run build:test-support && npm run build:memserver && npm run build:fastboot-modules",
    "prepack": "npm run build",
    "bin": "node --experimental-modules cli.js",
    "test": "node_modules/.bin/ava --verbose"
  },
  "devDependencies": {
    "@ember/test-helpers": "1.5.0",
    "@glimmer/resolver": "^0.4.3",
    "ava": "^2.4.0",
    "cors": "^2.8.5",
    "ember-cli-fastboot": "2.2.1",
    "ember-cli-test-loader": "^2.2.0",
    "ember-data": "3.13.1",
    "ember-devtools": "^6.0.0",
    "ember-load-initializers": "^2.1.1",
    "ember-qunit": "^4.5.1",
    "ember-resolver": "^5.2.1",
    "ember-source": "3.13.2",
    "esm": "^3.2.25",
    "intercept-stdout": "^0.1.2",
    "loader.js": "^4.7.0",
    "mber-head": "^0.1.0",
    "morgan": "^1.9.1",
    "qunit": "2.9.2",
    "strip-ansi": "^6.0.0",
    "whatwg-fetch": "^3.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/izelnakri/mber.git"
  },
  "dependencies": {
    "@babel/core": "^7.7.4",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-proposal-decorators": "^7.7.4",
    "@babel/plugin-transform-modules-amd": "^7.7.4",
    "@babel/plugin-transform-typescript": "^7.7.4",
    "@babel/preset-env": "^7.7.4",
    "@ember-data/canary-features": "^3.12.0",
    "@ember/ordered-set": "^2.0.3",
    "abortcontroller-polyfill": "^1.3.0",
    "amd-name-resolver": "^1.3.1",
    "ansi-colors": "^4.1.1",
    "babel-plugin-debug-macros": "^0.3.2",
    "babel-plugin-ember-modules-api-polyfill": "^2.9.0",
    "babel-plugin-filter-imports": "^3.0.0",
    "babel-plugin-htmlbars-inline-precompile": "^3.0.0",
    "babel-plugin-module-resolver": "^3.2.0",
    "babel6-plugin-strip-heimdall": "^6.0.1",
    "babelify": "^10.0.0",
    "browserify": "^16.3.0",
    "cheerio": "^1.0.0-rc.3",
    "chokidar": "^3.0.2",
    "compression": "^1.7.3",
    "eslint": "^6.7.0",
    "eslint-plugin-ember": "^7.1.0",
    "express": "^4.17.1",
    "fastboot": "^2.0.3",
    "fs-extra": "^8.1.0",
    "jquery": "^3.4.1",
    "jsdom": "^15.1.1",
    "klaw": "^3.0.0",
    "memserver": "^1.2.10",
    "mustache": "^3.0.1",
    "node-fetch": "^2.6.0",
    "ora": "^4.0.2",
    "puppeteer": "2.0.0",
    "sass": "1.23.7",
    "say": "^0.16.0",
    "terser": "4.4.0",
    "through2": "^3.0.0",
    "ws": "^7.1.1"
  },
  "ava": {
    "files": [
      "test/commands/*",
      "test/runners/*",
      "test/utils/*",
      "test/builders/*",
      "test/transpilers/*"
    ],
    "helpers": [
      "test/helpers/*"
    ],
    "babel": {
      "testOptions": {
        "inputSourceMap": false,
        "sourceMaps": false
      }
    },
    "require": [
      "esm",
      "./disable-colors"
    ],
    "concurrency": 1,
    "failFast": true
  },
  "volta": {
    "node": "12.4.0"
  }
}
